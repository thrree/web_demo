extends layout
block header
  style.
    .red{
    color:#ff0000;
    }
    ul{
    list-style-type: none;
    padding: 0px;
    margin: 0px;
    }
    li{
    padding-bottom: 2px;
    }
block content
  ul
    each obj in output_list
      if (obj.type == 'err')
        li.red!= obj.content
      else
        li!= obj.content




